[repo-local-build]
name=Repo Local Build $releasever - $basearch
baseurl=file:///src/repo/$basearch
enabled=1
deltarpm=0
metadata_expire=1
repo_gpgcheck=0
gpgcheck=0
type=rpm
skip_if_unavailable=False
module_hotfixes=1

[repo-local-source]
name=Repo Local Source $releasever - $basearch
baseurl=file:///src/repo/SRPMS
enabled=1
deltarpm=0
metadata_expire=1
repo_gpgcheck=0
gpgcheck=0
type=rpm
skip_if_unavailable=True
